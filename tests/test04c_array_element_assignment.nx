// TEST 04C: Atribuição de elemento inteiro do array
print("=== TEST 04C: Atribuição de elemento do array ===")

struct Point
    x: int,
    y: int
end

let points: Point[2] = [Point(10, 20), Point(30, 40)]
print("Before: Point 0 x = " + to_str(points[0].x))

// Tentar substituir o elemento inteiro ao invés do campo
let new_point: Point = Point(50, 20)
points[0] = new_point
print("After: Point 0 x = " + to_str(points[0].x))

print("✓ Test 04C passed")
