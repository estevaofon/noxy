
func main() -> void
    print("--- Testing Nested Lists ---")
    // Case 1: Nested Lists
    let list_of_lists: int[][] = [[1, 2], [3, 4]]
    print(f"Item [0][1]: {list_of_lists[0][1]}")

    print("--- Testing Nested Maps ---")
    // Case 2: Nested Maps
    let nested_map: map[string, map[string, int]] = {
        "group1": {"a": 1, "b": 2},
        "group2": {"c": 3}
    }
    print(f"Group1-a: {nested_map['group1']['a']}")

    print("--- Testing List of Maps ---")
    // Case 3: List of Maps
    let list_of_maps: map[string, int][] = [{"a": 1}, {"b": 2}]
    print(f"Item 0 'a': {list_of_maps[0]['a']}")

    print("--- Testing Map of Lists ---")
    // Case 4: Map of Lists
    let map_of_lists: map[string, int[]] = {
        "odds": [1, 3, 5],
        "evens": [2, 4, 6]
    }
    print(f"Odds[1]: {map_of_lists['odds'][1]}")

    print("--- Testing Struct with Complex Fields ---")
    // Case 5: Struct with nested list
    let matrix_container: MatrixContainer = MatrixContainer([[1, 0], [0, 1]])
    print(f"Matrix[0][0]: {matrix_container.data[0][0]}")
end

struct MatrixContainer
    data: int[][]
end

main()
