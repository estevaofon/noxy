
// math_lib.nx
// A simple example library for Noxy

struct MathConfig
    precision: int
end

func new_config(p: int) -> MathConfig
    return MathConfig(p)
end

func add(a: int, b: int) -> int
    return a + b
end

func subtract(a: int, b: int) -> int
    return a - b
end

func multiply(a: int, b: int) -> int
    return a * b
end
