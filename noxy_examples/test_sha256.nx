use crypto

let input: bytes = b"hello"
let hash: string = crypto.sha256(input)
print("Input: hello")
print("Hash:  " + hash)
print("Exp:   2cf24dba5fb0a30e26e83b2ac5b9e29e1b161e5c1fa7425e73043362938b9824")

if hash == "2cf24dba5fb0a30e26e83b2ac5b9e29e1b161e5c1fa7425e73043362938b9824" then
    print("PASS")
else
    print("FAIL")
end

