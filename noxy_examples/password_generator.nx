use rand select random_int

let letters: string = "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ"
let size: int = length(letters)
let i: int = 12
let password: string = ""


while i > 0 do
    let r2: int = random_int(1, size)
    let index: int = r2 - 1
    password = password + letters[index]
    i = i - 1
end
print(f"Password: {password}")