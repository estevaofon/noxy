// test_parser.nx


func main() -> void
    // Test case: A string containing an escaped JSON object
    // Source: "{\"msg\":\"hello\"}"
    let input: string = "\"{\\\"msg\\\":\\\"hello\\\"}\""
    
    print("Input: " + input)
    
    let val: any = json_parse(input)
    
    // In native json, a string input resolves to a string
    // Let's assume we can simply print it or check type if "type_of" existed.
    // For now just print.
    print("Parsed Val: " + to_str(val))
end

main()
