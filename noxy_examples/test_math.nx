// Módulo de teste para funções matemáticas

func add(a: int, b: int) -> int
    return a + b
end

func multiply(a: int, b: int) -> int
    return a * b
end

func power(base: int, exp: int) -> int
    let result: int = 1
    let i: int = 0
    while i < exp do
        result = result * base
        i = i + 1
    end
    return result
end

global pi: float = 3.14159
